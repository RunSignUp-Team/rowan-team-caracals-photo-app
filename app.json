{
  "expo": {
    "name": "media-app",
    "slug": "media-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/brand/logo-text.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/brand/logo-text.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/brand/logo-circle.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./src/assets/brand/logo-circle.png"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
