{
  "expo": {
    "name": "media-app",
    "slug": "media-app",
    "scheme": "media-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/brand/logo-text.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/brand/logo-text.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/brand/logo-circle.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/brand/logo-circle.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router"
    ]
  }
}
